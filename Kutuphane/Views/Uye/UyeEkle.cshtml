@model Kutuphane.Models.Entity.TBLUYELER
@{
    ViewBag.Title = "PersonelEkle";
    Layout = "~/Views/Shared/Layout.cshtml";
}

    <form class="form-group" method="post">
        <div></div>
        <div style="margin-top:15px">
            <label>Üye Adı</label>
            
            @Html.TextBoxFor(x => x.AD, new { @class = "form-control" })
            @Html.ValidationMessageFor(x => x.AD, "", new { @style = "color:forestgreen" })
        </div>
        <div style="margin-top:15px">
        </div>
        <div style="margin-top:15px">
            <label>Üye Soyadı</label>
            
            @Html.TextBoxFor(x => x.SOYAD, new { @class = "form-control" })
        </div>
        <div style="margin-top:15px"></div>
        <div style="margin-top:15px">
            <label>MAIL</label>
           
            @Html.TextBoxFor(x => x.MAIL, new { @class = "form-control" })
        </div>
        <div style="margin-top:15px"></div>
        <div style="margin-top:15px"></div>
        <div style="margin-top:15px">
            <label>Kullanıcı Adı</label>
         
            @Html.TextBoxFor(x => x.KULLANICIADI, new { @class = "form-control" })
        </div>
        <div style="margin-top:15px"></div>
        <div style="margin-top:15px">
            <label>SIFRE</label>
         
            @Html.TextBoxFor(x => x.SIFRE, new { @class = "form-control" })
        </div>
        <div style="margin-top:15px"></div>
        <div style="margin-top:15px">
            <label>Fotoğraf</label>
            
            @Html.TextBoxFor(x => x.FOTOGRAF, new { @class = "form-control" })
        </div>
        <div style="margin-top:15px"></div>
        <div style="margin-top:15px">
            <label>TELEFON</label>
           
            @Html.TextBoxFor(x => x.TELEFON, new { @class = "form-control" })
        </div>
        <div style="margin-top:15px"></div>
        <div style="margin-top:15px">
            <label>OKUL</label>
           
            @Html.TextBoxFor(x => x.OKUL, new { @class = "form-control" })
        </div>
        <div style="margin-top:15px"></div>
        <button class="btn btn-info" id="Btn1">Üye Ekle</button>
    </form>


